{
    "name": "netzmacht/php-javascript-builder",
    "description": "PHP Javascript builder library",
    "type": "library",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "David Molineus",
            "email": "david.molineus@netzmacht.de"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "netzmacht/contao-build-tools": "~1.1",
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~0.2",
        "symfony/event-dispatcher": "~2.3 || ~3.0 || ~4.0"
    },
    "suggest": {
        "symfony/event-dispatcher": "Install the symfony event dispatcher to use the event dispatching encoder."
    },
    "autoload": {
        "psr-0": {
            "Netzmacht\\Javascript": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
